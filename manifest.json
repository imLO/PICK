{
    "name":"Pick",
    "version":"0.0",
    "manifest_version":2,
    "description": "Image Bookmarking",
    "background":{
      "persistent": false,
      "scripts":["scripts/jquery.lazyload.min.js",
                 "scripts/content.js",
                 "scripts/background.js"]
    },
    "permissions":[
        "contextMenus",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action":{
      "default_icon":"img/icon16.png",
      "default_popup": "popup.html"
    },
    "icons": { 
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "web_accessible_resources":[
        "img/icon128.png"
    ],
    "author":"LO"
}